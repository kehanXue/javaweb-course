<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>CodeReturnServlet</servlet-name>
        <servlet-class>servlet.CodeReturnServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CodeReturnServlet</servlet-name>
        <url-pattern>/CodeReturnServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>SelectBeerServlet</servlet-name>
        <servlet-class>servlet.SelectBeerServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SelectBeerServlet</servlet-name>
        <url-pattern>/SelectBeer.do</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ShowBrownBeerServlet</servlet-name>
        <servlet-class>servlet.ShowBrownBeerServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ShowBrownBeerServlet</servlet-name>
        <url-pattern>/ShowBrownBeer.do</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ShowDarkBeerServlet</servlet-name>
        <servlet-class>servlet.ShowDarkBeerServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ShowDarkBeerServlet</servlet-name>
        <url-pattern>/ShowDarkBeer.do</url-pattern>
    </servlet-mapping>


</web-app>